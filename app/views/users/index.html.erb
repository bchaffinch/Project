<h1>List of Users (Index)</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
	<tr>
		<td>Name</td>
		<td>E-Mail</td>
		<td>Password</td>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td><%=user.name%></td>
			<td><%=user.email%></td>
			<td><%=user.password%></td>
		</tr>
	<% end %>
</table>


